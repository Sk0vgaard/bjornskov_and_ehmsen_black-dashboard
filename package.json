{
  "name": "black-dashboard-angular",
  "version": "1.2.0",
  "description": "Black Dasboard Angular, a Creative Tim Design. Coded by Creative Tim",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "precommit": "npm-run-all -s \"lint:fix\"",
    "lint": "npm-run-all -s \"lint:ts\" \"lint:scss\"",
    "lint:fix": "npm-run-all -s \"lint:ts:fix\" \"lint:scss:fix\" \"prettier:fix\" \"prettier-package:fix\"",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "lint:ts": "ng lint",
    "lint:ts:fix": "npm run lint -- --fix",
    "serve": "ng serve",
    "test:ci": "ng test --run-in-band",
    "test:watch": "ng test --watch",
    "prettier": "prettier --check \"**/*.{js,package.json,css,scss,less,md,ts,html,component.html}\"",
    "prettier-package:fix": "prettier-package-json --write ./package.json",
    "prettier:fix": "prettier --write \"**/*.{js,css,scss,less,md,ts,html,component.html}\"",
    "e2e": "ng e2e",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "build:gh": "ng build --configuration production --deploy-url=https://sk0vgaard.github.io/bjornskov_and_ehmsen_black-dashboard/ --base-href=",
    "copy404": "ln -s index.html 404.html",
    "homepage": "."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/black-dashboard-angular.git"
  },
  "keywords": [],
  "author": "Creative Tim <hello@creative-tim.com> (https://www.creative-tim.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creativetimofficial/black-dashboard-angular/issues"
  },
  "homepage": "https://demos.creative-tim.com/black-dashboard-angular/#/dashboard",
  "private": false,
  "dependencies": {
    "@angular/animations": "15.1.1",
    "@angular/common": "15.1.1",
    "@angular/compiler": "15.1.2",
    "@angular/core": "15.1.1",
    "@angular/forms": "15.1.1",
    "@angular/localize": "^15.1.1",
    "@angular/platform-browser": "15.1.1",
    "@angular/platform-browser-dynamic": "15.1.1",
    "@angular/router": "15.1.1",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@ng-bootstrap/ng-bootstrap": "^14.0.1",
    "chart.js": "2.9.4",
    "core-js": "3.15.2",
    "ngx-toastr": "^16.0.2",
    "npkill": "^0.10.0",
    "rxjs": "7.8.0",
    "swiper": "^8.4.6",
    "tslib": "2.4.1",
    "tslint-to-eslint-config": "^2.13.4",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.1.1",
    "@angular-eslint/builder": "15.2.0",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "15.2.0",
    "@angular-eslint/template-parser": "15.2.0",
    "@angular/cli": "15.1.1",
    "@angular/compiler-cli": "15.1.1",
    "@angular/language-service": "15.1.1",
    "@schuchard/prettier": "^5.1.0",
    "@types/jasmine": "3.8.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "16.3.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.49.0",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-change-detection-strategy": "^0.1.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.6.8",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "@popperjs/core": "2.11.6",
    "protractor": "7.0.0",
    "stylelint": "^14.4.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "ts-node": "10.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.9.4"
  },
  "lint-staged": {
    "*.{json,css,scss,less,md,html}": [
      "prettier --write"
    ],
    "*.component.html": [
      "prettier --write --parser html"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
